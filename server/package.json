{
  "name": "express-server",
  "version": "0.0.1",
  "description": "no",
  "main": "index.js",
  "repository": "https://github.com/stepanovmax/stepanovmax",
  "author": "Stepanov Max",
  "license": "MIT",
  "scripts": {
    "local-start": "NODE_ENV=localhost nodemon --exec babel-node server.js",
    "dev-start": "NODE_ENV=development nodemon --exec babel-node server.js",
    "stage-start": "NODE_ENV=stage nodemon --exec babel-node server.js",
    "start": "yarn build && node server.js",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "deb": "babel-node --presets es2015 app.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "sib-api-v3-sdk": "^8.2.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "nodemon": "^2.0.2"
  }
}
